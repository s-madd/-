= input(\"Игрок: \")\n","  comp = choice(s)\n","  print()\n","  print('Компьютер:', comp)\n","\n","  if player == 'камень' and comp == 'камень':\n","    print('Ничья!')\n","  if player == 'камень' and comp == 'ножницы':\n","    print('Победа: Игрок')\n","    pp += 1\n","  if player == 'камень' and comp == 'бумага':\n","    print('Победа: Компьютер')\n","    pc += 1\n","\n","  if player == 'ножницы' and comp == 'ножницы':\n","    print('Ничья!')\n","  if player == 'ножницы' and comp == 'камень':\n","    print('Победа: Компьютер')\n","    pc += 1\n","  if player == 'ножницы' and comp == 'бумага':\n","    print('Победа: Игрок')\n","    pp += 1\n","\n","  if player == 'бумага' and comp == 'бумага':\n","    print('Ничья!')\n","  if player == 'бумага' and comp == 'камень':\n","    print('Победа: Игрок')\n","    pp += 1\n","  if player == 'бумага' and comp == 'ножницы':\n","    print('Победа: Компьютер')\n","    pc += 1\n","  print()\n","  print('Комп:', pc, 'Игрок:', pp)\n","if pp > pc:\n","  print('Оончательная победа: Игрок')\n","else: print('Окончательная победа: Компьютер')"],"metadata":{"id":"ooIPCRe_JHkX","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1727868331757,"user_tz":-300,"elapsed":80875,"user":{"displayName":"Вячеслав","userId":"04005648150973531437"}},"outputId":"09b362a0-d01c-4896-8f03-104c4277c6d6"},"execution_count":64,"outputs":[{"output_type":"stream","name":"stdout","text":["Комп: 0 Игрок: 0\n","Раунд 1\n","Игрок: ножницы\n","\n","Компьютер: ножницы\n","Ничья!\n","\n","Комп: 0 Игрок: 0\n","Раунд 2\n","Игрок: ножницы\n","\n","Компьютер: ножницы\n","Ничья!\n","\n","Комп: 0 Игрок: 0\n","Раунд 3\n","Игрок: ножницы\n","\n","Компьютер: ножницы\n","Ничья!\n","\n","Комп: 0 Игрок: 0\n","Раунд 4\n","Игрок: ножницы\n","\n","Компьютер: бумага\n","Победа: Игрок\n","\n","Комп: 0 Игрок: 1\n","Раунд 5\n","Игрок: ножницы\n","\n","Компьютер: камень\n","Победа: Компьютер\n","\n","Комп: 1 Игрок: 1\n","Раунд 6\n","Игрок: ножницы